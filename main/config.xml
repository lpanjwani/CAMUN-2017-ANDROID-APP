<?xml version='1.0' encoding='utf-8'?>
<widget id="org.cambridgemun" version="0.9.beta" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>CAMUN 2017</name>
    <description>
        Founded in 2008, CAMUN, organised by the Cambridge High School (Abu Dhabi), is one of the foremost MUN clubs in the UAE.
    </description>
    <author email="lavesh@cambridgemun.org" href="https://www.cambridgemun.org">
        Lavesh Panjwani
    </author>
    <preference name="AndroidExtraFilesystems" value="files,files-external,documents,sdcard,cache,cache-external,assets,root" />
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="cdvfile:*" />
    <icon src="www/images/logo.png" />
    <platform name="android">
        <preference name="android-minSdkVersion" value="14" />
            <preference name="android-targetSdkVersion" value="23" />
    <allow-intent href="market:*" />
    </platform>
</widget>
